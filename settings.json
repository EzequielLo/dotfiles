{
  "workbench.colorTheme": "In Bed by 7pm",
  "workbench.iconTheme": "material-icon-theme",

  "window.zoomLevel": 1,
  "editor.wordWrap": "on",
  "files.autoSave": "onFocusChange",
  "window.restoreWindows": "none",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.renderWhitespace": "none",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.fontFamily": "'Fira Code'",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "workbench.preferredDarkColorTheme": "Solarized Light",
  "workbench.preferredHighContrastColorTheme": "Solarized Light",
  "workbench.preferredHighContrastLightColorTheme": "Solarized Light",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontFamily": "'Fira Code'",
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.leader": "<space>",
  "vim.neovimConfigPath": "~/.config/nvim/init.lua",
  "vim.neovimUseConfigFile": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "git.confirmSync": false,
  "workbench.activityBar.visible": false,
	"editor.insertSpaces": false,
	"window.menuBarVisibility": "toggle"
}
